{ "name": "Hypem",
  "version": "1.0",
  "description": "Control Hypem.com without having to find the tab",
  "browser_action": {
      "default_icon": "icon_16.png",
      "popup": "popup.html"
  },
  "icons": { 
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png" },
      "permissions": ["http://hypem.com/*","tabs" ]
}
